Proceso numero_dias
	
	Escribir "Ingrese un numero del 1 a 7"
	Leer num
	
	si num = 1
		Escribir "Lunes"
	SiNo
		si num = 2
			Escribir "Martes"	
		SiNo
			si num = 3
				Escribir "Miercoles"
			SiNo
				si num = 4
					Escribir "Jueves"
				SiNo
					si num = 5
						Escribir "Viernes"
					SiNo
						si num = 6
							Escribir "Sabado"
						SiNo
							si num = 7
								Escribir "Domingo"
							FinSi
						FinSi
					FinSi
				FinSi
			FinSi
		FinSi
		
	FinSi


FinProceso

